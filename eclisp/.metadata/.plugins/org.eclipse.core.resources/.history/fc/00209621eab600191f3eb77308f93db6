	public static int[] Sorf(int n, int[] mang1) {
		int tempt;
		for (int i = 0; i < n; i++) {
			for (int j = i+1; j < n; j++) {
				if (mang1[i]>mang1[j]) {
					tempt = mang1[j];
					mang1[j] = mang1[i];
					mang1[i] = tempt;
				}
				
			}
		}
		return mang1;
		
	}

}
